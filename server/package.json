{
  "name": "mna-catalogue-apprentissage-server",
  "version": "2.7.0",
  "description": "[MNA] App catalogue apprentissage",
  "repository": "https://github.com/mission-apprentissage/catalogue-apprentissage.git",
  "private": false,
  "author": "MNA",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon --ignore tests/ src/index.js",
    "debug": "nodemon --inspect=0.0.0.0 --signal SIGINT --ignore tests/ src/index.js",
    "test": "mocha --recursive --exit --timeout 5000 tests/",
    "lint": "eslint src/ tests/",
    "docker:start": "docker-compose up -d --build --force-recreate",
    "docker:stop": "docker-compose stop",
    "docker:down": "docker-compose down",
    "docker:clean": "docker-compose kill && docker system prune --force --volumes",
    "seed": "node src/jobs/seed/index.js",
    "sdk:playground": "node src/jobs/tcoSdkPlayground/index.js",
    "jobs": "node src/jobs/index.js",
    "jobs:test": "mocha --recursive --exit --timeout 60000 src/jobs/**/tests/",
    "clear": "node src/jobs/clear/index.js",
    "stats": "node src/jobs/stats/index.js",
    "rco:importer": "standalone=true node src/jobs/rcoImporter/index.js",
    "rco:converter": "standalone=true node src/jobs/rcoConverter/index.js",
    "trainings:updater": "standalone=true node src/jobs/trainingsUpdater/index.js",
    "psup:coverage-2020": "standalone=true node src/jobs/parcoursup/2020/coverage.js",
    "psup:import-2020": "standalone=true node src/jobs/parcoursup/2020/importer.js",
    "psup:coverage-2021": "standalone=true node src/jobs/parcoursup/2021/coverage.js",
    "psup:import-2021": "standalone=true node src/jobs/parcoursup/2021/importer.js",
    "psup:reconcile-2021": "standalone=true node src/jobs/parcoursup/2021/reconciliation.js",
    "psup:oneshot": "standalone=true node src/jobs/oneshot/psFormation/index.js",
    "psup:cleanup": "standalone=true node src/jobs/oneshot/psFormation/cleanup.js",
    "psup:missing": "standalone=true node src/jobs/oneshot/psFormation/missingEtablissement.js",
    "psup:reference": "standalone=true node src/jobs/reference/parcoursup/index.js",
    "psup:pertinence": "standalone=true node src/jobs/pertinence/parcoursup/index.js",
    "affelnet:coverage": "standalone=true node src/jobs/affelnet/coverage.js",
    "affelnet:import": "standalone=true node src/jobs/oneshot/afFormation/index.js",
    "affelnet:reference": "standalone=true node src/jobs/reference/affelnet/index.js",
    "affelnet:pertinence": "standalone=true node src/jobs/pertinence/affelnet/index.js",
    "affelnet:missing-cfd": "standalone=true node src/jobs/oneshot/afFormation/missingCfd.js",
    "affelnet:reconciliation": "standalone=true node src/jobs/affelnet/reconciliation.js",
    "es:index": "standalone=true node src/jobs/esIndex/index.js",
    "generate:swaggerschema": "standalone=true node src/jobs/generateSwaggerSchema/index.js",
    "etablissement:dump": "standalone=true node src/jobs/etablissements",
    "rcoPending:export": "standalone=true node src/jobs/pendingFormations/index.js",
    "rcoFormation:id": "standalone=true node src/jobs/oneshot/migrateRcoFormationIds/index.js"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7.6.1",
    "@mission-apprentissage/tco-service-node": "2.3.0",
    "axios": "0.21.1",
    "body-parser": "1.19.0",
    "boom": "7.3.0",
    "bunyan": "1.8.12",
    "bunyan-mongodb-stream": "0.0.3",
    "bunyan-prettystream": "0.1.3",
    "bunyan-slack": "0.0.10",
    "cli-progress": "3.8.2",
    "compose-middleware": "5.0.1",
    "config": "3.3.1",
    "connect-mongo": "3.2.0",
    "convert-csv-to-json": "0.0.15",
    "deep-object-diff": "^1.1.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "elasticsearch-scroll-stream": "1.2.1",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "fs-extra": "9.0.0",
    "full-icu": "1.3.1",
    "iconv-lite": "0.5.1",
    "joi": "17.2.1",
    "json2csv": "5.0.1",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.15",
    "luxon": "^1.25.0",
    "mjml": "^4.7.1",
    "moment": "2.26.0",
    "mongoose": "5.10.2",
    "mongoose-paginate": "5.0.3",
    "mongoose-schema-jsonschema": "1.4.4",
    "multipipe": "4.0.0",
    "nodemailer": "^6.4.14",
    "nodemailer-html-to-text": "^3.1.0",
    "oleoduc": "0.4.6",
    "passport": "0.4.1",
    "passport-http": "0.3.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "require-all": "3.0.0",
    "sha512crypt-node": "0.1.0",
    "string-similarity": "4.0.4",
    "swagger-jsdoc": "6.0.1",
    "swagger-ui-express": "4.1.6",
    "xlsx": "0.16.9"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "prettier": {
    "printWidth": 120,
    "bracketSpacing": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "axiosist": "0.9.0",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.3",
    "husky": ">=4",
    "lint-staged": ">=10",
    "mocha": "7.1.2",
    "nodemon": "2.0.3",
    "prettier": "2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./scripts/preventSensibleFilesCommit.sh && lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
